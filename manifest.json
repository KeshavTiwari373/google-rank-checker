{
    "name": "Rank Checker Extension",
    "version": "1.0",
    "description": "Check Google ranking for URLs.",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "clipboardRead",
      "storage",
      "alarms"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  